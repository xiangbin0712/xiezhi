<view class="home">
    <image class="locationA" src="{{themeA.entrance_img}}" />
    <swiper indicator-dots="{{true}}" indicator-active-color="#157058" autoplay="{{true}}" interval="{{5000}}" duration="{{500}}" circular="{{true}}">
        <block wx:for="{{bannerB.items}}">
            <swiper-item>
                <image class="swiper-image" src="{{item.img}}" />
            </swiper-item>
        </block>
    </swiper>
    <!-- grid -->
    <c-category-grid categoryC="{{categoryC}}" />
</view>